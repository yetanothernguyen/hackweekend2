@import "mixin";

.goaltop{
	background:#51D8EB;
	padding:20px;
	.title{
		font-family: museo;
		font-size: 17px;
		font-style: normal;
		font-weight: 300;
		margin: 0 0 10px;
	}
	.members{
		li{
			display:inline-table;
			margin:0 4px 0 0;
			&:last-child{
				margin:0;
			}
		}
	}
}


.interaction{
 	.conversation{
		float:left;
		width:200px;
		margin: 0 0 0 10px;
	}
}