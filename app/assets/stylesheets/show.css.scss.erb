@import "mixin";

.goaltop{
	background:#51D8EB;
	padding:20px;
	.title{
		font-size:16px;
		margin:0 0 10px;
	}
	.members{
		li{
			display:inline-table;
			margin:0 4px 0 0;
			&:last-child{
				margin:0;
			}
		}
	}
}


.interaction{
	height:333px;
	overflow-y:auto;
	.conversation{
		float:left;
		width:200px;
		margin: 0 0 0 10px;
	}
}